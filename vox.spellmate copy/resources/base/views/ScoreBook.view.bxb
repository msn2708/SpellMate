result-view {
  match: ScoreBook (this) {
  }
  render {
    
    layout {
      
      layout-macro (scores-layout) {
        param (scores) {
          expression (this.score)
        }
      }
      
      layout-macro (scores-calculation) {
        param (scoreBook) {
          expression (this)
        }
      }
    }
  }
}